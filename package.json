  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:smoke": "node tests/api.smoke.mjs",
    "test:load": "k6 run tests/load.k6.js",
    "smoke": "node tests/api.smoke.mjs",
    "format": "prettier --write .",
    "check": "npm run lint && npm run typecheck && npm run build",
    "validate": "npm run check && npm run test",
    "db:types": "echo 'Database types are auto-generated by Supabase'",
    "postinstall": "echo 'Dependencies installed successfully'",
    "api:dev": "tsx apps/api/src/index.ts",
    "api:start": "node apps/api/dist/index.js",
    "api:build": "tsc -p apps/api/tsconfig.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.930.0",
